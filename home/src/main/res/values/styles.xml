<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!-- 单独的MaterialButton样式-->
    <style name="Button" parent="Widget.MaterialComponents.Button">
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">@dimen/sp_14</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:insetTop">0dp</item>
        <item name="android:insetBottom">0dp</item>
        <item name="android:gravity">center</item>
        <item name="backgroundTintMode">src_atop</item>
        <item name="backgroundTint">@color/gray_e5e5ee</item>
        <item name="elevation">@dimen/dp_10</item>
        <item name="rippleColor">@color/white</item>
        <item name="iconTintMode">src_atop</item>
        <item name="iconTint">@color/white</item>
        <item name="iconGravity">textStart</item>
        <item name="iconPadding">0dp</item>
        <item name="iconSize">@dimen/dp_20</item>
    </style>

</resources>