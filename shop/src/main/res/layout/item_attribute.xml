<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <data>
        <variable
            name="model"
            type="String" />
    </data>
    <com.changanford.shop.view.TypefaceTextView
        android:layout_marginEnd="@dimen/dp_5"
        android:background="@drawable/shadow_e8ebf3_12dp"
        android:paddingStart="@dimen/dp_10"
        android:paddingEnd="@dimen/dp_10"
        android:gravity="center"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:minHeight="@dimen/dp_22"
        tools:text="红色"
        android:text="@{model}"
        android:maxWidth="@dimen/dp_90"
        style="@style/style_txt_ellipsize"
        android:textColor="@color/picture_color_66"
        android:textSize="@dimen/sp_11"
        />
</layout>