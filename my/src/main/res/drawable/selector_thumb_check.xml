<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_checked="true">
        <shape android:shape="oval">
            <size android:width="@dimen/dp_22" android:height="@dimen/dp_22" />
            <solid android:color="#01025c" />
            <stroke android:width="@dimen/dp_3" android:color="#dedfe4" />
        </shape>
    </item>
    <item android:state_checked="false">
        <shape android:shape="oval">
            <size android:width="@dimen/dp_22" android:height="@dimen/dp_22" />
            <solid android:color="#9196a5" />
            <stroke android:width="@dimen/dp_3" android:color="#dedfe4" />
        </shape>
    </item>

</selector>