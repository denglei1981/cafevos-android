<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:id="@+id/shape"
        android:bottom="1dp"
        android:left="-10dp"
        android:right="-10dp"
        android:top="-10dp">
        <shape>
            <solid android:color="@color/transparent" />//透明
            <stroke
                android:width="1dp"
                android:color="@color/teal_200" />//改变的颜色
        </shape>
    </item>
</layer-list>
