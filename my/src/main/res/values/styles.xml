<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="text_14_color_00_style">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">#333333</item>
        <item name="android:textSize">@dimen/sp_14</item>
    </style>

    <style name="line">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">1dp</item>
        <item name="android:background">#f2f2f2</item>
    </style>
    <style name="linelight">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">0.01dp</item>
        <item name="android:background">#EDEDED</item>
    </style>
    <style name="text_16_color_00_style" parent="text_14_color_00_style">
        <item name="android:textSize">@dimen/sp_16</item>
    </style>

    <style name="text_18_color_ff_style" parent="text_14_color_00_style">
        <item name="android:textSize">@dimen/sp_18</item>
        <item name="android:textColor">#ffffff</item>
    </style>

    <style name="login_mobile_et_style">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">@dimen/dp_44</item>
        <item name="android:layout_marginTop">@dimen/dp_40</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:textColor">#000000</item>
        <item name="android:paddingStart">@dimen/dp_8</item>
        <item name="android:layout_marginStart">@dimen/dp_20</item>
        <item name="android:layout_marginEnd">@dimen/dp_20</item>
        <item name="android:maxLines">1</item>
        <item name="android:textSize">@dimen/sp_14</item>
    </style>

    <style name="login_btn_style">
        <item name="android:textSize">@dimen/sp_15</item>
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">@dimen/dp_42</item>
        <item name="android:layout_marginStart">@dimen/dp_20</item>
        <item name="android:layout_marginTop">@dimen/dp_20</item>
        <item name="android:layout_marginEnd">@dimen/dp_20</item>
        <item name="android:background">@drawable/next_btn_bg</item>
        <item name="android:gravity">center</item>
        <item name="android:textColor">@color/white</item>
    </style>

    <style name="Toolbar.TitleText" parent="TextAppearance.Widget.AppCompat.Toolbar.Title">
        <item name="android:textSize">11sp</item>
        <item name="android:textColor">#000000</item>
    </style>

    <style name="text_car_info_lift_style">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginStart">@dimen/dp_20</item>
        <item name="android:layout_marginTop">@dimen/dp_21</item>
        <item name="android:textColor">#666666</item>
        <item name="android:textSize">@dimen/sp_13</item>
    </style>

    <style name="text_car_info_right_style">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginStart">@dimen/dp_20</item>
        <item name="android:layout_marginTop">@dimen/dp_21</item>
        <item name="android:layout_marginEnd">@dimen/dp_20</item>
        <item name="android:textColor">#333333</item>
        <item name="android:textSize">@dimen/sp_13</item>
    </style>

    <style name="line_car_info_style">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">@dimen/dp_1</item>
        <item name="android:layout_marginStart">@dimen/dp_20</item>
        <item name="android:layout_marginTop">@dimen/dp_12</item>
        <item name="android:layout_marginEnd">@dimen/dp_20</item>
        <item name="android:background">#EEEEEE</item>
    </style>

    <!-- 圆形图片 -->
    <style name="circleImageStyle">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">50%</item>
    </style>

    <!-- 圆角图片 -->
    <style name="roundedCornerImageStyle">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">@dimen/dp_5</item>
    </style>

    <style name="mine_checkbox" parent="@android:style/Widget.Material.CompoundButton.CheckBox">
        <item name="android:colorControlActivated">#FC883B</item>
        <item name="android:colorControlNormal">#AFB3B6</item>
    </style>

</resources>