<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="text_14_color_00_style">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">#333333</item>
        <item name="android:textSize">@dimen/sp_14</item>
    </style>

    <style name="text_16_color_00_style" parent="text_14_color_00_style">
        <item name="android:textSize">@dimen/sp_16</item>
    </style>

    <style name="text_18_color_ff_style" parent="text_14_color_00_style">
        <item name="android:textSize">@dimen/sp_18</item>
        <item name="android:textColor">#ffffff</item>
    </style>

    <style name="login_mobile_et_style">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">@dimen/dp_44</item>
        <item name="android:layout_marginTop">@dimen/dp_40</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:textColor">#000000</item>
        <item name="android:paddingStart">@dimen/dp_8</item>
        <item name="android:layout_marginStart">@dimen/dp_20</item>
        <item name="android:layout_marginEnd">@dimen/dp_20</item>
        <item name="android:maxLines">1</item>
        <item name="android:textSize">@dimen/sp_14</item>
    </style>

    <style name="login_btn_style">
        <item name="android:textSize">@dimen/sp_15</item>
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">@dimen/dp_46</item>
        <item name="android:layout_marginStart">@dimen/dp_20</item>
        <item name="android:layout_marginTop">@dimen/dp_20</item>
        <item name="android:layout_marginEnd">@dimen/dp_20</item>
        <item name="android:background">@drawable/next_btn_bg</item>
        <item name="android:gravity">center</item>
        <item name="android:textColor">@color/white</item>
    </style>

    <style name="Toolbar.TitleText" parent="TextAppearance.Widget.AppCompat.Toolbar.Title">
        <item name="android:textSize">11sp</item>
        <item name="android:textColor">#000000</item>
    </style>

</resources>