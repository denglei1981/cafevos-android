<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <data>

    </data>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">

        <com.changanford.common.ui.view.WTextView
            android:id="@+id/tv_service"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/dp_20"
            android:layout_marginTop="@dimen/dp_36"
            android:layout_marginEnd="@dimen/dp_20"
            android:gravity="center_vertical"
            android:textColor="@color/color_d916"
            android:textSize="18sp"
            app:layout_constraintTop_toBottomOf="@id/rv_car"
            tools:text="启程 享受购车服务" />

        <androidx.recyclerview.widget.RecyclerView
            android:id="@+id/rv_carService"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/dp_15"
            android:layout_marginTop="@dimen/dp_12"
            android:layout_marginEnd="@dimen/dp_15"
            app:layoutManager="androidx.recyclerview.widget.GridLayoutManager"
            app:layout_constraintTop_toBottomOf="@id/tv_service"
            app:spanCount="4"
            tools:itemCount="4"
            tools:listitem="@layout/item_car_service" />
    </LinearLayout>
</layout>