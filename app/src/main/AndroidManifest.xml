<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.changanford.evos">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name="com.changanford.common.MyApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:networkSecurityConfig="@xml/network_security_config"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.EVOS">
        <meta-data
            android:name="CHANNEL_VALUE"
            android:value="${channel_value}" />
        <activity
            android:name=".SplashActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.EVOS.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/Theme.EVOS.NoActionBar">
        </activity>
        <activity android:name="com.changanford.evos.com.changanford.circle.viewpager.Banner" />
        <activity android:name=".car.DetailActivity" />
        <activity android:name=".demo.DemoDetail" />
    </application>

</manifest>